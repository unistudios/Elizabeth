<graph
  animation="0"
  caption="Deployed By Date"
  xAxisName="Date"
  pyaxisname="Daily Install"
  syaxisname="Total Deployed"
  showNames="1"
  showvalues="1" 
  showalternatehgridcolor="1"
  alternatehgridcolor="CFB4D0"
  rotateNames="1"
  decimalPrecision="0"
  numdivlines="8"
  lineThickness="5"
  
>
  <categories>
  {% for object in object_list %}
    <category name ="{{object.install_date}}" />
  {% endfor %}
  </categories>
  
  <dataset color="1ECD15" parentYAxis="P">
  {% for object in object_list %}
    <set value='{{object.install_count}}' />
  {% endfor %}
  </dataset>
  
  <dataset color="D20808" parentYAxis="S">
  {% for t in totalbydate.items|dictsort:"0" %}
    <set value='{{t.1}}'/>
  {% endfor %}
  </dataset>
  
</graph> 