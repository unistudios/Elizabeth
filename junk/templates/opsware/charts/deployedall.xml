{% load charts %}
<graph
  caption="Overall Status" 
  animation="0"
  shownames="1" 
  showvalues="1" 
  showalternatehgridcolor="1"
  alternatehgridcolor="CFB4D0"
  numdivlines="8"
  divlinecolor="C4C5BD"
  decimalPrecision="0" 
  limitsDecimalPrecision="0"
  divLineDecimalPrecision="0"
  showcolumnshadow="0"
>
<categories>
{% for cat in xref.items.0.1.items %}  <category name="{{cat.0}}"/>
{% endfor %}
</categories>

{% for series in xref.items %}
  <dataset seriesName="{{series.0}}" showValues="1" color="{% ChartColor series.0 %}" >
      {% for values in series.1.items %}
        <set value="{{values.1}}" />
      {% endfor %}
  </dataset>
{% endfor %}
      

</graph>