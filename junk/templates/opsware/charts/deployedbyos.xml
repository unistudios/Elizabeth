<graph
  animation="0"
  caption="Deployed by OS"
  subcaption = "Total of {{total_servers}} servers"
  shownames="1"
  showvalues="1" 
  decimalprecision="0" 
  pieslicedepth="25"
  

  >
  {% for object in object_list %}
    <set name='{{object.os}}' hovertext='{{object.os}}' value='{{object.os_count}}' />
  {% endfor %}
</graph> 
